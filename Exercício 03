#Class Veiculo
class Veiculo:
    def __init__(self, tipo, velocidade):
        self.tipo = tipo
        self.velocidade = velocidade

#Class Carro
from Veiculo import Veiculo

class Carro(Veiculo):
    def __init__(self, tipo, velocidade):
        super().__init__(velocidade, tipo = "Carro")  
        self.velocidade = velocidade

#Class Moto
from Veiculo import Veiculo

class Moto(Veiculo):
    def __init__(self,velocidade, tipo):
        super().__init__(velocidade, tipo = "Moto")  
        self.velociade = velocidade
